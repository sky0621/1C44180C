// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  username String
  password String
  email    String @unique
}

model sessions {
  id         Int    @id @default(autoincrement())
  session_id String
  user_id    Int    @unique
}
